<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="
    	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
	default-lazy-init="false">
    
 	<dubbo:application name="${application.name}" owner="${application.owner}" />
 	
 	<dubbo:registry protocol="zookeeper" address="${registry.address}" /> 	
 	
 	<import resource="classpath*:META-INF/dubbo/biz-cnq-service.xml" />

	<dubbo:reference id="tokenServices" interface="com.appleframework.security.core.token.TokenServices" 
		version="1.0" check="false" />
		
	<dubbo:reference id="clientDetailsService" interface="com.appleframework.security.core.client.ClientDetailsService"
		version="1.0" check="false" />
</beans>